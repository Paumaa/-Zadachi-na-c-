using System; 

public class Program {

  public static void Main(string[] args) {
    int a = Convert.ToInt32(Console.ReadLine());
    if (a>0)
    {
      Console.WriteLine($"+");
    }
    else if (a<0)
    {
      Console.WriteLine($"-");
    }
    else {
      Console.WriteLine("NO");
    }
  }
}
