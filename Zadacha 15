using system;
using System.Collections.Generic;
using System.Linq;
using System.Threading.Tanks;

name space Sololearn{
 class Program{
  static void Main(string[] args){
   int M = int.Parse(Console.ReadLine());
   Console.WriteLine("*");
   for (int i = 0; i<M; i++){
    Console.Write('*');
    if (i > 0)
     for (int j = 0; j = 1 ; j++)
      if (i == M-1)
       Console.Write('*');
      else
       Console.Write(' ');
     Console.WriteLine('*');
    }
   }
  }
