using System; 

public class Program {
  public static void Main(string[] args) {
    int x=Convert.ToInt32(Console.ReadLine());
    int y=Convert.ToInt32(Console.ReadLine());
    int z=Convert.ToInt32(Console.ReadLine());
     if (x<=y && x<=z) 
    {
     Console.WriteLine(x);
    }
     else if (y<=x && y<=z){
      Console.WriteLine(y);
    }
     else 
     Console.WriteLine(z);
  }
}
